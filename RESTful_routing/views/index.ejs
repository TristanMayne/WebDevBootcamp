<%- include("partials/header") %>

<div class = "ui container main">
    <% blogs.forEach(function(blog){ %>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>" alt="">
            <span><%= blog.created %></span>
            <p><%= blog.body %></p>
            <a href="/blogs/<%= blog._id %> ">Show More</a>
        </div>
    <% }) %>
</div>

<%- include("partials/footer") %>
